{
  "info": {
    "name": "MediaFS",
    "_postman_id": "mediafs-collection-uuid",
    "description": "Postman collection for testing MediaFS API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "List Files",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer supersecret",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/files",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["files"]
        }
      },
      "response": []
    },
    {
      "name": "Get File",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer supersecret",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/files/{{filename}}",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["files", "{{filename}}"]
        }
      },
      "response": []
    },
    {
      "name": "Delete File",
      "request": {
        "method": "DELETE",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer supersecret",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/files/{{filename}}",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["files", "{{filename}}"]
        }
      },
      "response": []
    }
  ],
  "event": []
}